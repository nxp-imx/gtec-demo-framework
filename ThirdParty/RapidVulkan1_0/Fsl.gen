<?xml version="1.0" encoding="UTF-8"?>
<FslBuildGen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../FslBuildGen.xsd">
  <!-- While RapidVulkan is a header only library, we compile it as a normal static library
       so that we can do some extra exception message formatting via a custom CPP file -->
  <Library Name="RapidVulkan1_0" NoInclude="true">
    <Dependency Name="FslBase"/>
    <Dependency Name="Vulkan"/>
    <Define Name="FSL_DEMOFRAMEWORK" Access="Public"/>
    <Define Name="RAPIDVULKAN_ERRORFORMATTER_EXTERN" Access="Public"/>
    <Dependency Name="Recipe.RapidVulkan_1_0_68_0"/>
    <Platform Name="Windows" ProjectId="354EEE50-7F7B-44AD-94BB-3DD35938149C"/>
  </Library>
</FslBuildGen>
